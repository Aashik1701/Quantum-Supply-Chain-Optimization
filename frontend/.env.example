# Frontend Environment Variables Template

# API Configuration
# Legacy CRA style (if needed by some scripts)
REACT_APP_API_URL=http://localhost:5000
REACT_APP_WS_URL=ws://localhost:5000

# Vite style env variable actually read in code
VITE_API_BASE_URL=/api/v1

# External Services
REACT_APP_MAPBOX_TOKEN=pk.your_mapbox_access_token_here

# Feature Flags
REACT_APP_ENABLE_QUANTUM=true
REACT_APP_ENABLE_CLASSICAL=true
REACT_APP_ENABLE_HYBRID=true

# Development Settings
REACT_APP_DEBUG=false
GENERATE_SOURCEMAP=true

# Build Settings
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=10000
